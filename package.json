{
  "name": "tsdocs-monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.15.1",
  "description": "Monorepo for tsdocs",
  "scripts": {
    "build": "turbo build",
    "build:package": "turbo build --filter='./packages/*' --concurrency=1",
    "typecheck": "tsc --build tsconfig.json",
    "lint": "eslint .",
    "fix": "manypkg fix && eslint . --fix && prettier --write .",
    "docs": "turbo docs --concurrency 1",
    "change": "changeset",
    "ci:version": "changeset version && pnpm install --no-frozen-lockfile",
    "ci:publish": "pnpm run build:package && pnpm publish --access public -r --no-git-checks --tag latest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@manypkg/cli": "^0.25.1",
    "@ocavue/eslint-config": "^3.4.0",
    "eslint": "^9.34.0",
    "pkg-pr-new": "^0.0.60",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "^5.9.2"
  }
}
